# FRONTEND
PUBLIC_URL= # URL, по которому будет доступен фронтенд
REACT_APP_API_URL= # URL API бэкенда для отправки запросов

# BACKEND
JWT_SECRET= # Секретный ключ для подписи JWT токенов (случайная строка, например: your-super-secret-jwt-key-here)
JWT_EXPIRATION_TIME= # Время жизни JWT токена (например: 24h, 7d, 3600s)
PORT= # Порт, на котором будет запущен бэкенд сервер (например: 5000, 3001, 8080)
POSTGRES_PASSWORD= # Пароль для подключения к базе данных PostgreSQL
POSTGRES_DB= # Название базы данных PostgreSQL (например: myapp_db, production_db)
POSTGRES_USER= # Имя пользователя PostgreSQL (например: postgres, admin, myapp_user)
POSTGRES_HOST= # Хост базы данных PostgreSQL (например: localhost, db, postgres-container)
POSTGRES_PGDATA= # Путь к директории данных PostgreSQL внутри контейнера (обычно: /var/lib/postgresql/data)